{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "esModuleInterop": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "noEmit": true,
        "noImplicitAny": true,
        "target": "ESNext",
    },
    "include": [
        "./src/**/*"
    ]
}